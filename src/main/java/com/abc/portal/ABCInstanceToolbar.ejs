<%
if(value!=null){
%>
<div class="icon_toolbar">

	<div data-toggle="tooltip" data-placement="top" title="$ProcessView" class="icon-rounded m-r-10 tooltip-top" onclick="<%=methods.prcessView.caller()%>">
		<i class="glyphicon glyphicon-random workitem_btn"></i>
	</div>


	<div data-toggle="tooltip" data-placement="top" title="$WorkItems" class="icon-rounded m-r-10 tooltip-top" onclick="<%=methods.workItems.caller()%>">
		<i class="glyphicon glyphicon-inbox workitem_btn"></i>
	</div>


	<div data-toggle="tooltip" data-placement="top" title="<%=mw3.localize("$Pause")%>" class="icon-rounded m-r-10 tooltip-top"  onClick="<%=methods.suspend.caller()%>">
		<i class="glyphicon glyphicon glyphicon-pause workitem_btn"></i>
	</div>

	<div data-toggle="tooltip" data-placement="top" title="<%=mw3.localize("$Resume")%>" class="icon-rounded m-r-10 tooltip-top"  onClick="<%=methods.resume.caller()%>">
		<i class="glyphicon glyphicon glyphicon-play workitem_btn"></i>
	</div>

	<div data-toggle="tooltip" data-placement="top" title="<%=mw3.localize("Stop")%>" class="icon-rounded m-r-10 tooltip-top"  onClick="<%=methods.stop.caller()%>">
		<i class="glyphicon glyphicon glyphicon-stop workitem_btn"></i>
	</div>


	<div data-toggle="tooltip" data-placement="top" title="<%=mw3.localize("$Delete")%>" class="icon-rounded m-r-10 tooltip-top"  onClick="<%=methods.remove.caller()%>">
		<i class="glyphicon glyphicon-trash workitem_btn"></i>
	</div>



	<!-- Event Trigger -->
	<%
	if( value.eventTriggers ){
		for(var i=0; i < value.eventTriggers.length; i++){
			var options;
			var eventTrigger = value.eventTriggers[i];
			if(typeof options == 'undefined'){
				options = objectMetadata;
			}
			options['htmlTag'] =  'li';
	%>


	<div style="display:inline-block;border-radius: 25px;
    border: 1px solid #AAAAAA;
    padding: 15px;
    height: 40px; line-height: 10px; margin:
            0px;"><font color='#AAAAAA'><%=mw3.locateObject(eventTrigger, eventTrigger.__className, null , options, {objectId: objectId, name: '[' + i + ']'}) %></font></div>


	<%
		}
	}
}

%>